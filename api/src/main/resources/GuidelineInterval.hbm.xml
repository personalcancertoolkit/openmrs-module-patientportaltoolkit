<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.patientportaltoolkit">
    <class table="patientportal_guideline_interval" name="GuidelineInterval">
        <id name="id" type="int" column="id"  unsaved-value="0">
            <generator class="native" />
        </id>
        <property name="intervalLength" type="java.lang.Integer" column="interval_length" not-null="true"/>
        <many-to-one name="guideline" class="org.openmrs.module.patientportaltoolkit.Guideline" column="guideline_id" not-null="true"/>
    </class>
</hibernate-mapping>